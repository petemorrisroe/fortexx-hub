{
  "platform": {
      "apiRuntime": "python:3.10"
  },
  "routes": [
      {
          "route": "/",
          "rewrite": "/index.html"
      },
      {
          "route": "/api/*",
          "allowedRoles": ["anonymous"]
      }
  ],
  "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/api/*"]
  }
}


